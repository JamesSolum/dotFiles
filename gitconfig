# This is Git's per-user configuration file.
[user]
	name = <name>
	email = <email>
[alias]
	co = checkout
  st = status
  s = status
	cob = checkout -b
	ap = add -p
  up = !git pull --rebase --prune $@ && git submodule update --init --recursive
  b = branch
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
